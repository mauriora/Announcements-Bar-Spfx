        "clean-node-modules-az": "rimraf yarn.lock && yarn clean-node-modules"
    },
    "devDependencies": {
        "@mauriora/workspace-tools": "^0.0.4",
        "lerna": "^4.0.0",
        "rimraf": "^3.0.2",
        "yarn": "^1.0.0"
    },
    "dependencies": {
        "@mauriora/package-tools": "^0.0.4"
    },
    "main": "index.js",
    "author": "denniskuhn <dennis@kuhn.geek.nz>",
    "license": "MIT",
    "packageManager": "yarn@3.2.1"
}
