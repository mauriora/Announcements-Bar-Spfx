{
    "name": "@mauriora/rateable-announcements-solution",
    "version": "0.0.3",
    "repository": {
        "url": "https://github.com/mauriora/Rateable-Announcements-Solution.git"
    },
    "private": true,
    "workspaces": [
        "shared/**",
        "app/**"
    ],
    "scripts": {
        "serve": "yarn workspace @mauriora/rateable-announcements serve",
        "lint-app": "yarn workspace @mauriora/rateable-announcements lint",
        "build-debug": "yarn workspace @mauriora/rateable-announcements build-debug",
        "build-release": "yarn workspace @mauriora/rateable-announcements build-release",
        "build-shared": "yarn workspace @mauriora/controller-sharepoint-list build && yarn workspace @mauriora/utils-spfx-controls-react build",
        "clean-shared": "yarn workspace @mauriora/controller-sharepoint-list clean && yarn workspace @mauriora/utils-spfx-controls-react clean",
        "lint-shared": "yarn workspace @mauriora/controller-sharepoint-list lint && yarn workspace @mauriora/utils-spfx-controls-react lint",
        "clean-node-modules": "rmdir /s shared/Controller-SharePoint-List/node_modules && rmdir /s shared/Utils-SPFx-Controls-React/node_modules && rmdir /s app/Rateable-Announcements-Extension/node_modules && rmdir /s node_modules",
        "clean-all": "yarn workspace @mauriora/rateable-announcements clean && yarn clean-shared"
    },
    "devDependencies": {
        "yarn": "^1.0.0"
    },
    "dependencies": {}
}
