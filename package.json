{
    "name": "@mauriora/announcements-bar-spfx",
    "version": "0.1.4",
    "repository": "https://github.com/mauriora/Announcements-Bar-Spfx.git",
    "private": true,
    "workspaces": [
        "shared/**",
        "app/**"
    ],
    "scripts": {
        "serve": "yarn workspace @mauriora/announcements-bar-extension serve",
